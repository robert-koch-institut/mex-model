{
    "$$target": "/mex/model/entities/merged-contact-point#/identifier",
    "$id": "https://mex.rki.de/mex/model/entities/merged-contact-point",
    "$schema": "http://json-schema.org/draft/2020-12/schema",
    "additionalProperties": false,
    "description": "A mail address, where a group of people has access to.",
    "properties": {
        "email": {
            "description": "The email address associated to the contact point.",
            "items": {
                "examples": [
                    "info@rki.de"
                ],
                "format": "email",
                "pattern": "^[^@ \\t\\r\\n]+@[^@ \\t\\r\\n]+\\.[^@ \\t\\r\\n]+$",
                "type": "string"
            },
            "minItems": 1,
            "sameAs": [
                "http://www.w3.org/2006/vcard/ns#hasEmail",
                "https://schema.org/email"
            ],
            "type": "array"
        },
        "identifier": {
            "$ref": "/mex/model/fields/identifier",
            "description": "An unambiguous reference to the resource within a given context. Persistent identifiers should be provided as HTTP URIs ([DCT, 2020-01-20](http://dublincore.org/specifications/dublin-core/dcmi-terms/2020-01-20/)).",
            "readOnly": true,
            "sameAs": [
                "http://purl.org/dc/elements/1.1/identifier"
            ]
        },
        "supersededBy": {
            "anyOf": [
                {
                    "$ref": "/mex/model/entities/merged-contact-point#/identifier"
                },
                {
                    "type": "null"
                }
            ],
            "default": null,
            "description": "A merged item which is the preferred duplicate, because it replaces, consolidates or otherwise makes the current merged item obsolete."
        }
    },
    "required": [
        "email",
        "identifier"
    ],
    "sameAs": [
        "https://schema.org/ContactPoint"
    ],
    "title": "Merged Contact Point",
    "type": "object"
}
