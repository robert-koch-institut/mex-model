{
    "$$target": "/mex/model/entities/merged-primary-source#/identifier",
    "$id": "https://mex.rki.de/mex/model/entities/merged-primary-source",
    "$schema": "http://json-schema.org/draft/2020-12/schema",
    "additionalProperties": false,
    "description": "A collection of information, that is managed and curated by an RKI unit and lists activities and/or resources.",
    "properties": {
        "alternativeTitle": {
            "default": [],
            "description": "An alternative name for the primary source",
            "items": {
                "$ref": "/mex/model/fields/text"
            },
            "sameAs": [
                "http://purl.org/dc/terms/alternative"
            ],
            "type": "array"
        },
        "contact": {
            "default": [],
            "description": "An agent that serves as a contact for the primary source.",
            "items": {
                "anyOf": [
                    {
                        "$ref": "/mex/model/entities/merged-organizational-unit#/identifier"
                    },
                    {
                        "$ref": "/mex/model/entities/merged-person#/identifier"
                    },
                    {
                        "$ref": "/mex/model/entities/merged-contact-point#/identifier"
                    }
                ]
            },
            "sameAs": [
                "http://www.w3.org/ns/dcat#contactPoint"
            ],
            "type": "array"
        },
        "description": {
            "default": [],
            "description": "A short description of the primary source.",
            "items": {
                "$ref": "/mex/model/fields/text"
            },
            "sameAs": [
                "http://purl.org/dc/terms/description"
            ],
            "type": "array"
        },
        "documentation": {
            "default": [],
            "description": "A link to a document documenting the primary source.",
            "items": {
                "$ref": "/mex/model/fields/link"
            },
            "subPropertyOf": [
                "http://purl.org/dc/terms/isReferencedBy"
            ],
            "type": "array"
        },
        "identifier": {
            "$ref": "/mex/model/fields/identifier",
            "description": "An unambiguous reference to the resource within a given context. Persistent identifiers should be provided as HTTP URIs ([DCT, 2020-01-20](http://dublincore.org/specifications/dublin-core/dcmi-terms/2020-01-20/)).",
            "readOnly": true,
            "sameAs": [
                "http://purl.org/dc/elements/1.1/identifier"
            ]
        },
        "locatedAt": {
            "default": [],
            "description": "A URL that leads to the primary source or a filepath, where the primary source is stored.",
            "items": {
                "$ref": "/mex/model/fields/link"
            },
            "type": "array"
        },
        "supersededBy": {
            "anyOf": [
                {
                    "$ref": "/mex/model/entities/merged-primary-source#/identifier"
                },
                {
                    "type": "null"
                }
            ],
            "default": null,
            "description": "A merged item which is the preferred duplicate, because it replaces, consolidates or otherwise makes the current merged item obsolete."
        },
        "title": {
            "default": [],
            "description": "The name of the primary source.",
            "items": {
                "$ref": "/mex/model/fields/text"
            },
            "sameAs": [
                "http://purl.org/dc/terms/title"
            ],
            "type": "array"
        },
        "unitInCharge": {
            "default": [],
            "description": "This property refers to agents who assume responsibility and accountability for the primary source and its appropriate maintenance.",
            "items": {
                "$ref": "/mex/model/entities/merged-organizational-unit#/identifier"
            },
            "sameAs": [
                "http://dcat-ap.de/def/dcatde/maintainer"
            ],
            "type": "array"
        },
        "version": {
            "anyOf": [
                {
                    "examples": [
                        "v1",
                        "2023-01-16",
                        "Schema 9"
                    ],
                    "type": "string"
                },
                {
                    "type": "null"
                }
            ],
            "default": null,
            "description": "The version of the primary source, e.g. the date of the last modification."
        }
    },
    "required": [
        "identifier"
    ],
    "sameAs": [
        "http://www.w3.org/ns/dcat#Catalog",
        "http://www.w3.org/ns/prov#PrimarySource"
    ],
    "title": "Merged Primary Source",
    "type": "object"
}
